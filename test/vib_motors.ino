
#define dot1 23
#define dot2 22
#define dot3 21
#define dot4 19
#define dot5 18
#define dot6 5



void setup() {
    pinMode(dot1, OUTPUT);
    pinMode(dot2, OUTPUT);
    pinMode(dot3, OUTPUT);
    pinMode(dot4, OUTPUT);
    pinMode(dot5, OUTPUT);
    pinMode(dot6, OUTPUT);
}

void loop() {
  digitalWrite(dot1, HIGH);
  digitalWrite(dot2, HIGH);
  digitalWrite(dot3, HIGH);
  digitalWrite(dot4, HIGH);
  digitalWrite(dot5, HIGH);
  digitalWrite(dot6, HIGH);   
                                           
}